.data-list {
    overflow: hidden;
}
.data-list .items {
    width: calc(25% - 60rpx);
    float: left;
    padding: 30rpx;
}
.items-content {
  border-radius: 50%;
  padding: 20rpx;
  text-align: center;
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
}
.data-list .items image {
  width: 60rpx !important;
  height: 60rpx !important;
  margin-top: 5rpx;
}
.data-list .items .title {
    margin-top: 10rpx;
    font-size: 32rpx;
}